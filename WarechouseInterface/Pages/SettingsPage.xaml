<Window x:Class="WarechouseInterface.Pages.SettingsPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WarechouseInterface.Pages"
        mc:Ignorable="d"
        Title="SettingsPage" Height="200" Width="150">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>

        <Button Grid.Row="0" Margin="10" Content="Główny Widok" Click="Button_Click"/>
        <Button Grid.Row="1" Margin="10" Content="Ustawienia Hasła"/>
        <Button Grid.Row="2" Margin="10" Content="Wybór Magazynu"/>
    </Grid>
</Window>
